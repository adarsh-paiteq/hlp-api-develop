<!-- header -->
<table
  width='100%'
  cellspacing='0'
  cellpadding='0'
  border='0'
  role='presentation'
>
  <tbody>
    <tr>
      <td
        class='o_bg-light o_px-xs o_pt-lg o_xs-pt-xs'
        align='center'
        style='
background-color: white;
/* padding-left: 8px;
padding-right: 8px;
padding-top: 32px; */
'
      >
        <table
          class='o_block'
          width='100%'
          cellspacing='0'
          cellpadding='0'
          border='0'
          role='presentation'
          style='max-width: 632px; margin: 0 auto'
        >
          <tbody>
            <tr>
              <td
                class='o_bg-white o_px-md o_py-xl o_xs-py-md o_sans o_text-md o_text-light'
                align='center'
                style='
font-family: Poppins, sans-serif;
margin-top: 0px;
margin-bottom: 0px;
font-size: 19px;
line-height: 28px;
background-color: #ffffff;
color: #82899a;
padding-left: 24px;
padding-right: 24px;
padding-top: 47px;
padding-bottom: 50px;
'
              >
                <table
                  cellspacing='0'
                  cellpadding='0'
                  border='0'
                  role='presentation'
                >
                  <tbody>
                    <tr>
                      <td
                        class='o_sans o_text o_text-white o_bg-primary o_px o_py o_br-max'
                        align='center'
                        style='
font-family: Poppins, sans-serif;
margin-top: 0px;
margin-bottom: 0px;
font-size: 16px;
line-height: 24px;
padding-left: 16px;
padding-right: 16px;
padding-bottom: 0px;
'
                      >
                        <a href='https://imgbb.com/'><img
                            src='https://i.ibb.co/Yh1bXRn/Frame-14442.png'
                            width='103'
                            alt=''
                            style='
-ms-interpolation-mode: bicubic;
vertical-align: middle;
border: 0;
line-height: 100%;
height: auto;
outline: none;
text-decoration: none;
'
                          />
                        </a>
                      </td>
                    </tr>
                  </tbody>
                </table>
                <p
                  class='o_heading o_text-dark o_mb-xxs'
                  style='
font-family: Poppins, sans-serif;
font-weight: bold;
margin-top: 0px;
margin-bottom: 4px;
color: #242b3d;
font-size: 16px;
line-height: 39px;
'
                >
                {{h1}}
                </p>
                <h1
                  style='
font-size: 35px;
line-height: 42px;
margin-top: 0px;
margin-bottom: 0px;
color: #383d42;
font-weight: 500;
'
                >
                  {{title}}
                </h1>
                  <img
                            src='{{videoThumbnail}}'
                            width="400"
                            alt=''
                            style='
-ms-interpolation-mode: bicubic;
vertical-align: middle;
border: 0;
width:400px;
height: auto;
line-height: 100%;
outline: none;
text-decoration: none;
text-align: center;
    margin-top: 48px;
    margin-bottom: 9px;
'
                          />
              </td>
            </tr>
          </tbody>
        </table>
      </td>
    </tr>
  </tbody>
</table>

<!-- button-success -->
<table
  width='100%'
  cellspacing='0'
  cellpadding='0'
  border='0'
  role='presentation'
>
  <tbody>
    <tr>
      <td
        class='o_bg-light o_px-xs'
        align='center'
        style='
background-color: white;
padding-left: 8px;
padding-right: 8px;
'
      >
        <table
          class='o_block'
          width='100%'
          cellspacing='0'
          cellpadding='0'
          border='0'
          role='presentation'
          style='max-width: 632px; margin: 0 auto'
        >
          <tbody>
            <tr>
              <td
                class='o_bg-white o_px-md o_py-xs'
                align='center'
                style='
background-color: #ffffff;
padding-left: 24px;
padding-right: 24px;
padding-top: 0px;
padding-bottom: 0px;
'
              >
                <table
                  align='center'
                  cellspacing='0'
                  cellpadding='0'
                  border='0'
                  role='presentation'
                >
                  <tbody>
                    <tr>
                      <td
                        width='300'
                        class='o_btn o_bg-success o_br o_heading o_text'
                        align='center'
                        style='
font-family: Poppins, sans-serif;
font-weight: bold;
margin-top: 0px;
margin-bottom: 0px;
font-size: 16px;
line-height: 24px;
height: 55px;
mso-padding-alt: 12px 24px;
background-color: #6048de;
border-radius: 15px;
'
                      >
                        <a
                          class='o_text-white'
                          href={{videoLink}}
                          style='
text-decoration: none;
outline: none;
color: #ffffff;
display: block;
padding: 12px 24px;
mso-text-raise: 3px;
'
                        >{{button}}
                          <img
                            src='https://i.ibb.co/bNWwTMd/Circled-Left-Arrow.png'
                            alt='Image'
                            align='right'
                            style='
width: 24px;
height: 24px;
'
                          /></a>
                      </td>
                    </tr>
                  </tbody>
                </table>
              </td>
            </tr>
          </tbody>
        </table>
      </td>
    </tr>
  </tbody>
</table>
